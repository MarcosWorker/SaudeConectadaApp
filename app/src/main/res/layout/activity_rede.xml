<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.alphadev.saudeconectadaapp.flow.RedeActivity"
    android:background="@drawable/background_image">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="200dp" />
    <TextView
        android:id="@+id/text_activity_rede"
        android:text="Profissionais que atendem nesse local"
        android:layout_width="match_parent"
        android:gravity="center"
        android:textStyle="bold"
        android:padding="10dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/map"
        android:fontFamily="sans-serif-condensed"/>

    <ListView
        android:id="@+id/list_medicos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp"
        android:layout_below="@+id/text_activity_rede">

    </ListView>

</RelativeLayout>




